er2cds C_INSPLOTRESULTVALUERSLTRECG

entity I_INSPECTIONRESULTVALUE {
    alias _INSPECTIONRESULTVALUE
    key INSPECTIONLOT : NUMC
    key INSPPLANOPERATIONINTERNALID : NUMC
    key INSPECTIONCHARACTERISTIC : NUMC
    key INSPRESULTVALUEINTERNALID : NUMC
    INSPECTIONSUBSETINTERNALID : NUMC as InspectionSubsetInternalID
    INSPECTOR : CHAR
    INSPECTIONRESULTMEASUREDVALUE : FLTP as InspectionResultMeasuredValue
    INSPECTIONRESULTORIGINALVALUE : CHAR as InspectionResultOriginalValue
    CHARACTERISTICATTRIBUTECODEGRP : CHAR
    CHARACTERISTICATTRIBUTECODE : CHAR
    CHARCATTRIBUTECODEVERS : CHAR
    INSPECTIONVALUATIONRESULT : CHAR
    INSPECTIONRESULTITEM : CHAR
    INSPRESULTITEMINTERNALID : NUMC
}

entity I_INSPECTIONLOT {
    alias _INSPECTIONLOT
    PLANT : CHAR
    INSPECTIONLOTTYPE : CHAR
    MATLQUALITYAUTHORIZATIONGROUP : CHAR
    no-out INSPECTIONLOT : NUMC
}

entity I_QLTYMGMTPLANTLVLCONFIGN {
    alias _CONFIGURATION
    INSPSPECADDLDECIMALPLACES : INT1 as InspSpecTotalDecimalPlaces
    no-out PLANT : CHAR
}

entity I_INSPECTIONCHARACTERISTIC {
    alias _InspectionCharacteristic
    no-out INSPECTIONLOT : NUMC
    no-out INSPPLANOPERATIONINTERNALID : NUMC
    no-out INSPECTIONCHARACTERISTIC : NUMC
}

entity C_INSPLOTOPCHARCRSLTRECG {
    alias _InspChar
    no-out INSPECTIONLOT : NUMC
    no-out INSPPLANOPERATIONINTERNALID : NUMC
    no-out INSPECTIONCHARACTERISTIC : NUMC
    no-out MATERIALSAMPLE : CHAR
}

entity C_INSPVALUATIONRSLTVH {
    alias _InspectionValuationRsltVH
    no-out INSPECTIONVALUATIONRESULT : CHAR
}

entity C_INSPLOTRSLTRECG {
    alias _InspLot
    no-out INSPECTIONLOT : NUMC
}

relationship null {
    I_INSPECTIONRESULTVALUE[1] -> I_INSPECTIONLOT[1]
    join order 8
    INSPECTIONLOT = INSPECTIONLOT
}

relationship null {
    I_INSPECTIONLOT[1] -> I_QLTYMGMTPLANTLVLCONFIGN[0..N]
    join order 9
    PLANT = PLANT
}

association-to-parent relationship null {
    I_INSPECTIONRESULTVALUE[1] -> C_INSPLOTOPCHARCRSLTRECG[1]
    INSPECTIONLOT = INSPECTIONLOT
    INSPPLANOPERATIONINTERNALID = INSPPLANOPERATIONINTERNALID
    INSPECTIONCHARACTERISTIC = INSPECTIONCHARACTERISTIC
}

association relationship null {
    I_INSPECTIONRESULTVALUE[1] -> I_INSPECTIONCHARACTERISTIC[1]
    INSPECTIONLOT = INSPECTIONLOT
    INSPPLANOPERATIONINTERNALID = INSPPLANOPERATIONINTERNALID
    INSPECTIONCHARACTERISTIC = INSPECTIONCHARACTERISTIC
}

association relationship null {
    I_INSPECTIONRESULTVALUE[0..N] -> C_INSPVALUATIONRSLTVH[1]
    INSPECTIONVALUATIONRESULT = INSPECTIONVALUATIONRESULT
}

association relationship null {
    I_INSPECTIONRESULTVALUE[0..N] -> C_INSPLOTRSLTRECG[1]
    INSPECTIONLOT = INSPECTIONLOT
}