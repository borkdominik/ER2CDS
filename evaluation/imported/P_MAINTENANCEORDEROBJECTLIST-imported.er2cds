}

association relationship null {
    I_MAINTENANCEORDEROBJECTLIST[0..N] -> I_FUNCTIONALLOCATIONTEXT[0..N]
    FUNCTIONALLOCATION = FUNCTIONALLOCATION
}

association relationship null {
    I_MAINTENANCEORDEROBJECTLIST[0..N] -> I_MAINTENANCEORDER[0..N]
