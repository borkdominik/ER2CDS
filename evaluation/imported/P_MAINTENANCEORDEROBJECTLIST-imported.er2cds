}

entity I_TECHOBJISEQUIPORFUNCNLLOC {
    alias _TechObjIsEquipOrFuncnlLoc
    no-out TECHOBJISEQUIPORFUNCNLLOC : CHAR
}

association relationship null {
    I_MAINTENANCEORDEROBJECTLIST[0..N] -> I_EQUIPMENTTEXT[0..N]
    EQUIPMENT = EQUIPMENT
}

association relationship null {
    I_MAINTENANCEORDEROBJECTLIST[0..N] -> I_FUNCTIONALLOCATIONLABEL[1]
