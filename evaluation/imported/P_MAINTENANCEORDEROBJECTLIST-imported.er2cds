    EQUIPMENT : CHAR as TechnicalObject
    FUNCTIONALLOCATION : CHAR as TechnicalObject
    EQUIPMENT : CHAR as TechnicalObject
    EQUIPMENT : CHAR as TechObjIsEquipOrFuncnlLoc
    FUNCTIONALLOCATION : CHAR as TechObjIsEquipOrFuncnlLoc
    EQUIPMENT : CHAR as TechObjIsEquipOrFuncnlLoc
    EQUIPMENT : CHAR
    FUNCTIONALLOCATION : CHAR
    EQUIPMENT : CHAR as TechnicalObjectLabel
    FUNCTIONALLOCATION : CHAR as TechnicalObjectLabel
    EQUIPMENT : CHAR as TechnicalObjectLabel
    EQUIPMENT : CHAR as TechnicalObjectLabel
    EQUIPMENT : CHAR as TechnicalObjectDescription
    FUNCTIONALLOCATION : CHAR as TechnicalObjectDescription
    EQUIPMENT : CHAR as TechnicalObjectDescription
}

entity I_MAINTENANCEORDER {
    alias _MaintenanceOrder
    no-out MAINTENANCEORDEROBJECTLIST : INT8
}

entity I_FUNCTIONALLOCATIONLABEL {
    alias _FunctionalLocationLabel
    no-out FUNCTIONALLOCATION : CHAR
}

entity I_EQUIPMENTTEXT {
    alias _EquipmentText
    no-out EQUIPMENT : CHAR
}

entity I_FUNCTIONALLOCATIONTEXT {
    alias _FunctionalLocationText
