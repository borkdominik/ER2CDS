er2cds I_INSURCLMCLSREASON

entity I_INSURCLMCLOSUREREASON {
    key INSURCLMTYPE : CHAR
    key INSURCLMPOLICYPRODGENERATION : NUMC
    key INSURCLMPOLICYPRODUCTVERSION : NUMC
    key INSURCLMSUBCLMTYPE : CHAR
    key INSURCLMLIFECYCSTS : CHAR
    key INSURCLMSTATREASON : CHAR
}

entity I_INSURANCECLAIM {
    alias _Claim
    no-out INSURANCECLAIM : CHAR
    no-out INSURCLMTYPE : CHAR
    no-out INSURCLMTYPEGENERATION : NUMC
    no-out INSURCLMTYPEVERS : NUMC
    no-out INSURCLMLIFECYCSTS : CHAR
}

entity I_INSURCLMSTATSREASON {
    alias _ClsRsn
    no-out INSURCLMSTATREASON : CHAR
}

association relationship null {
    I_INSURCLMCLOSUREREASON[1] -> I_INSURANCECLAIM[1]
    INSURCLMTYPE = INSURCLMTYPE
    INSURCLMPOLICYPRODGENERATION = INSURCLMTYPEGENERATION
    INSURCLMPOLICYPRODUCTVERSION = INSURCLMTYPEVERS
    INSURCLMLIFECYCSTS = INSURCLMLIFECYCSTS
}

association relationship null {
    I_INSURCLMCLOSUREREASON[1] -> I_INSURCLMSTATSREASON[1]
    INSURCLMSTATREASON = INSURCLMSTATREASON
}